.jobs-index__wrapper.row
  .small-12.columns
    .jobs-index__tag-list#tag-list
      .job-tags-list
        - link = jobs_path
        = react_component("JobBoard", { roles: @roles, types: @types,
          link: link })
    //------ Cards -------//
    .jobs-index__card-list#jobs-index
      = render partial: "jobs",
        locals: { jobs: @jobs, favorites: @favorites }
/= render "shared/screen_modal"
